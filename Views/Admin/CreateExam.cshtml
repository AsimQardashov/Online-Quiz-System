
@{
    ViewBag.Title = "CreateExam";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
    // Layout = null;
}


<form action="~/Admin/CreateExam" method="post">
    <select class="fenler" name="CQId">
        @foreach (System.Data.DataRow item in ViewBag.categories.Rows)
        {
            <option value="@item["CQId"]">@item["CQName"]</option>
        }
    </select>
    <button type="submit">Basla</button>

    
</form>

